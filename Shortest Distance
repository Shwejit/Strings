#include <iostream>
#include <cmath>
using namespace std;

float shortestpath(string s){
int x=0, y=0;
for(int i=0;i<s.length();i++) {
if (s[i]=='N')
y=y+1;
else if (s[i]=='S') {
y=y-1;
}
else if (s[i]=='W') {
x=x-1;
}
else if (s[i]=='E') {
x=x+1;
}
}
int x2=xx;
int y2=yy;
return sqrt(x2+y2);
}

int main() {
string s="NS";
cout << shortestpath(s) << endl;
return 0;
}



